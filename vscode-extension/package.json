{
  "name": "kyco",
  "displayName": "KYCo",
  "description": "Send code selections to KYCo backend",
  "version": "0.1.0",
  "publisher": "kyco",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "kyco.sendSelection",
        "title": "KYCo: Send Selection"
      }
    ],
    "keybindings": [
      {
        "command": "kyco.sendSelection",
        "key": "ctrl+shift+c",
        "mac": "cmd+shift+c"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/vscode": "^1.85.0",
    "typescript": "^5.3.0"
  }
}
